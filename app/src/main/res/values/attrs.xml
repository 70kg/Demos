<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RangeSeekBar">
        <attr name="mode" format="integer"/>
        <attr name="min_value" format="integer"/>
        <attr name="max_value" format="integer"/>
        <attr name="init_min_value" format="integer"/>
        <attr name="init_max_value" format="integer"/>
    </declare-styleable>

    <attr name="title_text" format="color"/>

    <declare-styleable name="AvatarImageBehavior">
        <attr name="finalYPosition" format="dimension"/>
        <attr name="startXPosition" format="dimension"/>
        <attr name="startToolbarPosition" format="dimension"/>
        <attr name="startHeight" format="dimension"/>
        <attr name="finalHeight" format="dimension"/>
    </declare-styleable>
</resources>